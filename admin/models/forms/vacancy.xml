<?xml version="1.0" encoding="utf-8"?>
<form>
	<fieldset>
		<field
				name="id"
				type="hidden"
				/>
		
                
                <field
				name="floorplan_id"
				type = "modal_floorplan"
				
                                label="COM_PARKWAY_VACANCY_FLOORPLANID_LABEL"
				description="COM_PARKWAY_VACANCY_FLOORPLANID_DESC"
				
				>
                                <option value="">Choose One</option>
                </field>
                
                
                
                <field
				name="suite"
				type="text"
				label="COM_PARKWAY_VACANCY_TITLE_LABEL"
				description="COM_PARKWAY_VACANCY_TITLE_DESC"
				size="40"
				class="inputbox"
				default=""
				/>
                <field
				name="alias"
				type="text"
				label="COM_PARKWAY_VACANCY_ALIAS_LABEL"
				description="COM_PARKWAY_VACANCY_ALIAS_DESC"
				size="40"
				class="inputbox"
				default=""
				/>
                <field
				name="tooltip"
				type="text"
				label="COM_PARKWAY_VACANCY_TOOLTIP_LABEL"
				description="COM_PARKWAY_VACANCY_TOOLTIP_DESC"
				size="40"
				class="inputbox"
				default=""
				/>
                <field
				name="image"
				type="file"
				label="COM_PARKWAY_VACANCY_IMAGE_LABEL"
				description="COM_PARKWAY_VACANCY_IMAGE_DESC"
				size="40"
				class="inputbox"
				default=""
				/>
                <field id="published"
			name="published"
			type="list"
			label="JSTATUS"
			description="JFIELD_PUBLISHED_DESC"
			class="chzn-color-state"
			size="1"
			default="1"
		>
			<option value="1">
				JPUBLISHED</option>
			<option value="0">
				JUNPUBLISHED</option>
			<option value="2">
				JARCHIVED</option>
			<option value="-2">
				JTRASHED</option>

		</field>
                
                <field
				name="available_space"
				type="number"
				label="COM_PARKWAY_VACANCY_AVAILABLESPACE_LABEL"
				description="COM_PARKWAY_VACANCY_AVAILABLESPACE_DESC"
				size="40"
				class="inputbox validate-numeric required"
				default=""
				/>
                 <field
				name="divisible"
				type="radio"
				label="COM_PARKWAY_VACANCY_DIVISIBLE_LABEL"
				description="COM_PARKWAY_VACANCY_DIVISIBLE_DESC"
				size="40"
				class="btn-group btn-group-yesno"
				default=""
				>
                                <option value="1">JYES</option>
                                <option value="0">JNO</option>
                </field>
                 
                  <field
				name="market_rent"
				type="text"
				label="COM_PARKWAY_VACANCY_MARKETRENT_LABEL"
				description="COM_PARKWAY_VACANCY_MARKETRENT_DESC"
				size="40"
				class="inputbox"
				default=""
                                
				/>
                 
                              
                                
                <field
				name="date_available"
				type="calendar"
				label="COM_PARKWAY_VACANCY_DATEAVAILABLE_LABEL"
				description="COM_PARKWAY_VACANCY_DATEAVAILABLE_DESC"
				size="40"
				class="calendar"
				default="0000-00-00"
				/>
                
                <field
				name="pdf"
				type="file"
				label="COM_PARKWAY_VACANCY_PDF_LABEL"
				description="COM_PARKWAY_VACANCY_PDF_DESC"
				size="40"
				class="media"
				default=""
				/>
                
                
                
	</fieldset>
</form>

