<?xml version="1.0" encoding="utf-8"?>
<form>
	<fieldset>
		<field
				name="id"
				type="hidden"
				/>
		
                
                <field
				name="building_id"
				type="sql"
				query="SELECT id, name FROM #__parkway_buildings" key_field="id" value_field="name"
                                label="COM_PARKWAY_VACANCY_BUILDINGID_LABEL"
				description="COM_PARKWAY_VACANCY_BUILDINGID_DESC"
				size=""
				class=""
				default=""
				>
                                <option value="">Choose One</option>
                </field>
                <field
				name="floorplan_id"
				type="sql"
				query="SELECT id, title FROM #__parkway_floorplans" key_field="id" value_field="name"
                                label="COM_PARKWAY_VACANCY_FLOORPLANID_LABEL"
				description="COM_PARKWAY_VACANCY_FLOORPLANID_DESC"
				size=""
				class=""
				default=""
				>
                                <option value="">Choose One</option>
                </field>
                
                <field
				name="floor"
				type="text"
				label="COM_PARKWAY_VACANCY_FLOOR_LABEL"
				description="COM_PARKWAY_VACANCY_FLOOR_DESC"
				size="40"
				class="inputbox"
				default=""
				/>
                <field
				name="suite"
				type="text"
				label="COM_PARKWAY_VACANCY_SUITE_LABEL"
				description="COM_PARKWAY_VACANCY_SUITE_DESC"
				size="40"
				class="inputbox"
				default=""
				/>
                <field
				name="available_space"
				type="number"
				label="COM_PARKWAY_VACANCY_AVAILABLESPACE_LABEL"
				description="COM_PARKWAY_VACANCY_AVAILABLESPACE_DESC"
				size="40"
				class="inputbox validate-numeric required"
				default=""
				/>
                 <field
				name="divisible"
				type="radio"
				label="COM_PARKWAY_VACANCY_DIVISIBLE_LABEL"
				description="COM_PARKWAY_VACANCY_DIVISIBLE_DESC"
				size="40"
				class="btn-group btn-group-yesno"
				default=""
				>
                                <option value="1">JYES</option>
                                <option value="0">JNO</option>
                </field>
                 
                  <field
				name="market_rent"
				type="number"
				label="COM_PARKWAY_VACANCY_MARKETRENT_LABEL"
				description="COM_PARKWAY_VACANCY_MARKETRENT_DESC"
				size="40"
				class="validate-numeric required"
				default=""
                                
				/>
                <field
				name="pdf"
				type="file"
				label="COM_PARKWAY_VACANCY_PDF_LABEL"
				description="COM_PARKWAY_VACANCY_PDF_DESC"
				size="40"
				class="media"
				default=""
				/>                
                                
                <field
				name="date_available"
				type="calendar"
				label="COM_PARKWAY_VACANCY_DATEAVAILABLE_LABEL"
				description="COM_PARKWAY_VACANCY_DATEAVAILABLE_DESC"
				size="40"
				class="calendar"
				default="0000-00-00"
				/>
                
                
                
                
                
	</fieldset>
</form>

